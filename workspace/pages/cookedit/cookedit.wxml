<!--pages/cookedit/cookedit.wxml-->
<!--pages/cook/cookinformation/cook.wxml-->
<view class="itemView">登录账号：
  <view>
    <input class="input" name="account"  bindinput="account" />
  </view>
</view>
<view class="itemView">姓名：
  <view>
    <input class="input" name="cookname"  bindinput="setCookname" />
  </view>
</view>
<view class="itemView">电话：
  <view>
    <input class="input" name="cooktel"  type="tel" maxlength="11" bindinput="setCooktel" />
  </view>
</view>
<view class="itemView">是否经过培训：
  <view>
    <input class="input" name="cookpei"  bindinput="setCookpei" />
  </view>
</view>
<view class="itemView">是否有健康证：
  <view>
    <input class="input" name="cookhealth"  type="tel" maxlength="11" bindinput="setCookhealth" />
  </view>

</view>
<button type="primary" bindtap="subMission">保存并提交</button>
