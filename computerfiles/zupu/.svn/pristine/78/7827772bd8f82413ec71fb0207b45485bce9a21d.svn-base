<?xml version="1.0" encoding="utf-8"?>

<div xmlns="http://www.w3.org/1999/xhtml" component="$UI/system/components/justep/window/window" design="device:m;"
  xid="window" class="window">  
  <div component="$UI/system/components/justep/model/model" xid="model" style="top:757px;left:403px;height:auto;"
    onLoad="modelLoad"/>  
  <div component="$UI/system/components/justep/panel/panel" class="x-panel x-full"
    xid="panel1"> 
    <div class="x-panel-top" xid="top1"> 
      <div component="$UI/system/components/justep/titleBar/titleBar" title="我的预约"
        class="x-titlebar title"> 
        <div class="x-titlebar-left"> 
          <a component="$UI/system/components/justep/button/button" label=""
            class="btn btn-link btn-only-icon" icon="icon-chevron-left" onClick="{operation:'window.close'}"
            xid="backBtn"> 
            <i class="icon-chevron-left"/>  
            <span/> 
          </a> 
        </div>  
        <div class="x-titlebar-title"><![CDATA[我的预约]]></div>  
        <div class="x-titlebar-right reverse"/> 
      </div> 
    </div>  
    <div class="x-panel-content" xid="content1"> 
      <div class="app"> 
        <div class="list_container" v-for="data in datas" > 
          <div class="media_cont"> 
            <div> 
              <span class="sx">事项名称：</span>  
              <span id="matter_name" class="media_color" v-text="data.name"></span> 
            </div>  
            <div> 
              <span class="sx">事项编号：</span>  
              <span id="matter_number" class="media_color" v-text="data.code"></span> 
            </div>  
            <div> 
              <span class="sx">申请时间：</span>  
              <span id="application_time " class="media_color" v-text="data.uq_date"></span> 
            </div> 
          </div>  
          <div class="media_img"> 
            <img id="album" src="$UI/zupu/img/sh_succ.png" class="image_"/> 
          </div>  
          <div class="media_state"> 
            <a id="material" class="media_right" bind-click="material">已上传的材料</a>  
            <br/>  
            <a id="matter" class="media_right" bind-click="matter">查看事项</a>  
            <br/>  
            <div id="examine_details" class="media_right popup" data-type="audit">审核详情</div> 
          </div> 
        </div> 
      </div> 
    </div> 
  </div> 
</div>
