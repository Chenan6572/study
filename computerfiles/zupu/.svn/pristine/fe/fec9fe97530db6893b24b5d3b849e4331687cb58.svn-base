*{
	margin:0;
	padding:0;
}
.ui_home {
	background-color: rgba(242, 242, 242, 1);
	height: 30%;
	position: relative;
}

.search {
	padding-top: 3vh;
	padding-left:4%;
	display: flex;
	align-items: center;
}

.search_container {
	width: 4.2vw;
	height: 4.8vh;
}

.top_background {
	width: 100%;
	height: 70%;
	background-color: rgba(231, 64, 58, 1);
	border-radius: 0 0 6vw 6vw;
}

.click_work {
	height:60%;
	background-color: white;
	position: absolute;
	left: 1.5%;
	right:1.5%;
	top: 32%;
	border-radius: 3vw 3vw 3vw 3vw;
	display: flex;
	align-items: center;
	justify-content: space-around;
	flex-grow: 0.25;
}
.click_box_ {
	display: flex;
	flex-direction: column;
	width: 30vw;
	height: 12vh;
	align-items: center;
	justify-content: space-around;
}
.image_container_ {
	width: 15vw;
	height: 15vw;
	padding:0vh 0vh 3vw 0vw; 
}


.image_container {
	width: 10vw;
	height: 10vw;
}

.image_container_element {
	width: 100%;
	height: 100%;
}

.serve {
	display: flex;
	flex-direction: column;
	border-bottom: 2vh solid rgba(242, 242, 242, 1);
}

.click_ {
	display: flex;
	justify-content: space-around;
	flex-grow: 0.25;
}

.click_box {
	display: flex;
	flex-direction: column;
	width: 25vw;
	height: 10vh;
	align-items: center;
	justify-content: space-around;
}
.size_ {
	padding-left:2vw;
	font-size: 5vw;
	color: white;
	font-weight:800;
}
.size_small {
	font-size: 5vw;
}
.size_normal{
	font-size: 3vw;
}
.up {
	position: relative;
	left: 38%;
	width: 24%;
	border-top: 6vw solid rgba(231, 64, 58, 1);
	border-right: 4vw solid white;
	border-left: 4vw solid white;
	border-bottom: 4vw solid white;
}

.down {
	text-align: center;
}

.center {
	position: absolute;
	text-align: center;
	
	top: -3vh;
	
	color: white;
	font-size: 4vw;
}