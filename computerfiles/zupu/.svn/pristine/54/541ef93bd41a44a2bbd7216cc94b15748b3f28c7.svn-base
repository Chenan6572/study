* {
	margin:0px;
	padding:0px;
}

.title{
	background-color:rgba(231, 64, 58, 1);
}
._left {
	width:30%;
	position:absolute;
	background:rgba(228, 228, 228, 1);
}
ul {
	list-style:none;
}
li {
	
	height:60px;
	font-size:20px;
	border-bottom:rgba(228, 228, 228, 1) solid 1px;
	border-right:rgba(228, 228, 228, 1) solid 1px;
	line-height:60px;
	text-align:center;
	color:black;
	cursor:default;
	
}
.p {
	width:65%;
	height:100%;
	position:absolute;
	top:0;
	left:32%;
	display:none;
}
.bg_color {
	background:white;
	color: black;
}
.line{
	height:60px;line-height:60px;text-align:center; border-bottom:rgba(228, 228, 228, 1) solid 1px;
}